{
	"manifest_version": 2,

	"name": "Grid Builder",
	"description": "This extension apllies a grid to current web page.",
	"version": "1.0",

	"browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
	},

	"content_scripts": [
		{
		  "matches": ["*://*/*"],
		  "css": ["content.min.css"],
		  "js": ["jquery-2.1.1.min.js", "content.min.js"]
		}
	],

	"permissions": [
		"storage"
	]
}
